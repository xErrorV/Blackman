{
    "name": "GigaNiga",
    "version": "1.0",
    "author": "BlackMan",
    "manifest_version": 3,
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["index.js"]
        }
    ],
    "host_permissions": ["<all_urls>"]
}
